{
  "name": "@fxcie/utils",
  "version": "0.0.1",
  "description": "Francis Carelse's Utilities",
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "bash -c \"npx tsc src/*.ts --outDir lib --lib es2017 --declaration\"",
    "postbuild": "bash -c \"npx tsc src/*.ts --outDir lib/mjs --target es2017 --declaration\"",
